{
  "name": "@cenk1cenk2/react-template",
  "private": true,
  "description": "Scaffolding for react/next.js templates.",
  "author": {
    "name": "Cenk Kilic",
    "email": "cenk@kilic.dev",
    "url": "https://srcs.kilic.dev"
  },
  "version": "0.0.0-development",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx run-many --target=serve --parallel --maxParallel=1000 --all",
    "start:one": "nx serve",
    "build": "nx run-many --target=build --all",
    "build:one": "nx build",
    "build:nocache": "yarn build --skip-nx-cache",
    "test": "nx run-many --target=test --parallel --maxParallel=1000 --all",
    "test:one": "nx test",
    "test:e2e": "nx run-many --target=e2e --parallel --maxParallel=1000 --all",
    "lint": "nx format:write --all && nx workspace-lint && yarn lint:check --fix",
    "lint:check": "nx run-many --target=lint --all --",
    "lint:one": "nx lint",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx format:write && nx affected:lint --all",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "clean": "rimraf dist/ .cache/ node_modules/ yarn.lock yarn-error.log package-lock.json"
  },
  "workspaces": {
    "packages": [
      "apps/*",
      "libs/*"
    ],
    "nohoist": []
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn affected:lint",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "dependencies": {
    "document-register-element": "1.13.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-is": "16.13.1",
    "react-router-dom": "5.2.0",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@cenk1cenk2/eslint-config": "^0.4.19",
    "@nrwl/cli": "10.3.1",
    "@nrwl/cypress": "10.3.1",
    "@nrwl/eslint-plugin-nx": "10.3.1",
    "@nrwl/jest": "10.3.1",
    "@nrwl/react": "^10.3.1",
    "@nrwl/web": "10.3.1",
    "@nrwl/workspace": "10.3.1",
    "@testing-library/react": "10.4.1",
    "@types/jest": "26.0.8",
    "@types/node": "~14.14.6",
    "@types/react": "16.9.38",
    "@types/react-dom": "16.9.8",
    "@types/react-is": "16.7.1",
    "@types/react-router-dom": "5.1.5",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "babel-jest": "26.2.2",
    "babel-plugin-styled-components": "1.10.7",
    "cypress": "^4.1.0",
    "dotenv": "8.2.0",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "husky": "^4.3.0",
    "jest": "26.2.2",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "ts-jest": "26.4.0",
    "ts-node": "~9.0.0",
    "tslint": "~6.1.3",
    "typescript": "~4.0.3"
  }
}
